{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-reference": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  }
}
